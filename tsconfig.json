{
    "compilerOptions": {
        "importHelpers": true,
      "target": "ES2020",
      "module": "ESNext",
      "moduleResolution": "node",
      "isolatedModules": true,
      "esModuleInterop": true,
      "jsx": "react",
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "dist",
      "baseUrl": ".",
      "paths": {
        "*": ["node_modules/*"]
      }
    },
    "include": ["src"]
  }
  